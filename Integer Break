class Solution {
        public:
                int integerBreak(int n) {
                        int test=1;
                        if(n==0||n==1) return n;
                        if(n==2) return 1;
                        if(n==3) return 2;
                        while(n>4){
                                n=n-3;
                                test*=3;
                        }
                        return test*n;





                }
};